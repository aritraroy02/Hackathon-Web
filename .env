# Frontend Environment Variables for Child Health PWA

# Backend API URL - Google Cloud Run Production URL
REACT_APP_BACKEND_URL=https://child-health-backend-747316458447.us-central1.run.app

# For development, use local backend:
# REACT_APP_BACKEND_URL=http://localhost:8080

# App Configuration
REACT_APP_NAME=Child Health PWA
REACT_APP_VERSION=1.0.0

# PWA Configuration
REACT_APP_OFFLINE_MODE=true
REACT_APP_DEMO_MODE=true
REACT_APP_PWA_NAME=Child Health Tracker
REACT_APP_PWA_SHORT_NAME=ChildHealth
REACT_APP_PWA_DESCRIPTION=Offline-first child health data collection PWA

# Feature Flags
REACT_APP_ENABLE_LOCATION_SERVICES=true
REACT_APP_ENABLE_PHOTO_UPLOAD=true
REACT_APP_ENABLE_OFFLINE_MODE=true

# Development
GENERATE_SOURCEMAP=false
REACT_APP_DEBUG_MODE=false
